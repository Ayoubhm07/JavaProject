<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane fx:id="mainComponent" minHeight="-Infinity" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.carte.gui.back.SideBarController">
    <VBox layoutX="16.0" spacing="15.0" stylesheets="@../../utils/MainStyle.css" AnchorPane.bottomAnchor="15.0"
          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="15.0">
        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Espace admin" wrappingWidth="180">
            <font>
                <Font name="System Bold" size="26.0"/>
            </font>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </Text>
        <Button fx:id="btnComptes" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                mnemonicParsing="false" onAction="#afficherComptes" text="Comptes" textFill="WHITE">
            <HBox.margin>
                <Insets/>
            </HBox.margin>
            <font>
                <Font name="Segoe UI Bold" size="15.0"/>
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button fx:id="btnTransactions" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                mnemonicParsing="false" onAction="#afficherTransactions" text="Transactions" textFill="WHITE">
            <HBox.margin>
                <Insets/>
            </HBox.margin>
            <font>
                <Font name="Segoe UI Bold" size="15.0"/>
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button fx:id="btnCarteBancaires" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                mnemonicParsing="false" onAction="#afficherCarteBancaires" text="CarteBancaires" textFill="WHITE">
            <HBox.margin>
                <Insets/>
            </HBox.margin>
            <font>
                <Font name="Segoe UI Bold" size="15.0"/>
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button fx:id="btnTypes" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                mnemonicParsing="false" onAction="#afficherTypes" text="Types carte" textFill="WHITE">
            <HBox.margin>
                <Insets/>
            </HBox.margin>
            <font>
                <Font name="Segoe UI Bold" size="15.0"/>
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>

    </VBox>
    <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
            onAction="#logout" style="-fx-background-color: red; -fx-background-radius: 0;" text="Logout"
            textFill="WHITE"
            AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <font>
            <Font name="Segoe UI Bold" size="15.0"/>
        </font>
        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
    </Button>
</AnchorPane>
